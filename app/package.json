{
    "name": "electron_template",
    "version": "1.0.0",
    "description": "Description",
    "repository": {},
    "main": "main.js",
    "scripts": {
        "install": "cmake-js compile",
        "start": "electron .",
        "packager": "electron-packager . --asar --overwrite=true --icon=build_resources/icons/main_icon.png",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "directories": {
            "output": "build",
            "buildResources": "build_resources"
        },
        "files": [
            "**/*",
            "build/Release"
        ],
        "icon": "build_resources/icons/main_icon.png",
        "win": {
            "target": "appx",
            "certificateFile": "build_resources/selfsigncert.pfx",
            "certificatePassword": "password"
        },
        "linux": {
            "category": ""
        },
        "appx": {
            "identityName": "000pub.app",
            "publisher": "CN=000",
            "publisherDisplayName": "pub",
            "applicationId": "app",
            "displayName": "app"
        }
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "node-addon-api": "^4.1.0"
    },
    "devDependencies": {
        "cmake-js": "^6.2.1",
        "electron": "^13.2.3",
        "electron-builder": "^22.11.7",
        "electron-packager": "^15.3.0"
    }
}
